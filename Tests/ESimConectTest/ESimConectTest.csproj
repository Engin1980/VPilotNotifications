<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
		<UseWPF>true</UseWPF>
    <TargetFramework>net6.0-windows</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PlatformTarget>x64</PlatformTarget>
    <AssemblyVersion>2.5.0.1</AssemblyVersion>
    <FileVersion>1.5.0.1</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="ESimConnect">
      <HintPath>..\..\_DLLs\_NET_CORE_6\ESimConnect.dll</HintPath>
    </Reference>
    <Reference Include="Microsoft.FlightSimulator.SimConnect">
      <HintPath>..\..\_DLLs\_NET_CORE_6\Microsoft.FlightSimulator.SimConnect.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
